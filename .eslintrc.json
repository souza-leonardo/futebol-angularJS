{
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "script"
  },
  "extends": [
    "plugin:angular/johnpapa"
  ],
  "rules": {
    "angular/controller-as-route": 0,
    "angular/controller-name": 0,
    "angular/file-name": 0,
    "angular/function-type": 0,
    "angular/module-getter": 0,
    "angular/no-run-logic": 0,
    "angular/no-service-method": 0,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-function": 2,
    "no-extra-semi": 2,
    "no-irregular-whitespace": 2,
    "no-multi-spaces": 2,
    "semi": 2,
    "semi-spacing": 2,
    "semi-style": 2,
    "valid-typeof": 2
  },
  "env": {
    "browser": true
  }
}
